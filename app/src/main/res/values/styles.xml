<?xml version="1.0" encoding="utf-8"?>
<!--suppress AndroidDomInspection -->
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.DarkActionBar">
    </style>

    <style name="MyCoolStyle" parent="AppTheme">
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:textColor">@color/design_default_color_background</item>
        <item name="android:layout_height">@dimen/layout_height</item>
        <item name="android:layout_width">@dimen/layout_width</item>
        <item name="android:textStyle">italic</item>
    </style>

    <style name="AppThemeLight" parent="Theme.MaterialComponents.Light">
        <item name="colorPrimary">#e91e63</item>
        <item name="colorPrimaryDark">#b0003a</item>
        <item name="colorAccent">#ff6090</item>
    </style>

    <style name="AppThemeDark" parent="Theme.MaterialComponents">
    </style>

</resources>